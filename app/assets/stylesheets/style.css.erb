body {
  margin: 0 auto;  
}

header {
  background-color: #333;
  width: 100%;
  top: 0px;
  min-height: 80px;
  /*position: fixed;*/
  z-index: 300;
  padding-top: 25px;
}

.center {
  max-width: 80em;
  margin: 0 auto;
}

#navbar ul {
  list-style: none;
}

#navbar ul li {
  display: inline;
}

.welcome_links {
  color: blue;
}

.welcome_links:visited {
  color: orange;
}

/*style individual list items*/
#navbar-ul li {
  list-style-type: none;
  display: inline;
}

/*style ul navbar */
#navbar-ul {
  background-color: #FF9000;
  padding: 0;
  margin: 0;
  height: 50px;
  margin-top: -5px; /* for some reason, small space between img and navbar */

}

