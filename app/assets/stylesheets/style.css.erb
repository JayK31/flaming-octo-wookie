body {
  background: url("index.jpg") no-repeat center;
  background-size: cover;
  color: white;
}

.welcome_links {
  color: blue;
}

.welcome_links:visited {
  color: orange;
}

.top-bar-section li {
  color: white;
  text-align: center;
}

.index h1 {
  text-align: center;
  color: white;
}

.index {
  height: 850px;
  width: 100%;
  background: url("index.jpg") no-repeat center;
  background-size: cover;
}

.name h1 {
  color: white;
}

.name h1 a:not(.button):hover {
background: black;
}

.small-5.columns {
  padding-top: 100px;
  color: white;
}

  
.sign-up-page, .login-page {
  height: 850px;
  width: 100%;
  text-align: center;
}

.sign-up-page, .login-page h2 {
  padding: 10px 0;
  margin: 0;
}

.sign-up-page p {
  text-align: center;
  padding: 0 0 5px 0;
}

.sign-up-form, .login-form {
  padding: 0;
  margin: 0 auto;
  text-align: center;
  margin-left: 475px;
}

.sign-up-form input, .login-form input {
  border-radius: 5px;
}

